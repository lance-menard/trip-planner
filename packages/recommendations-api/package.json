{
  "name": "recommendations-api",
  "version": "1.0.0",
  "description": "A graphQL API serving recommendations for a user.",
  "main": "dist/index.js",
  "author": "Lance Menard",
  "license": "MIT",
  "private": true,
  "scripts": {
    "watch": "cross-env HTTP_LISTENING_PORT=4002 gulp watch",
