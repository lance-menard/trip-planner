    "cross-fetch": "^3.1.4",
    "convict": "^5.2.0",
    "graphql": "^14.6.0",
    "graphql-import": "^1.0.2",
    "graphql-iso-date": "^3.6.1",
    "graphql-middleware": "^4.0.3",
    "graphql-tools": "^4.0.8",
    "graphql-type-json": "^0.3.1",
    "ramda": "^0.26.1",
    "ramda-adjunct": "^2.25.0",
    "source-map-support": "^0.5.16",
    "verror": "^1.10.0",
    "cross-var": "^1.1.0"
  },
  "devDependencies": {
    "gulp": "^4.0.2"
